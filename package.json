{
  "name": "tihlde-pythons",
  "version": "7.0.0",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "next dev",
    "build": "yarn generate && next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --fix src",
    "studio": "npx prisma studio",
    "generate": "npx prisma generate",
    "push": "npx prisma db push",
    "migrate": "npx prisma migrate dev --name",
    "fixtures": "npx fixtures ./src/fixtures",
    "createdb": "docker run --name event_registration -e POSTGRES_PASSWORD=docker -p 5432:5432 -d postgres",
    "fresh": "docker rm -f -v event_registration && yarn createdb && sleep 1 && yarn push && yarn generate && yarn fixtures"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.7",
    "@mui/material": "^5.14.7",
    "@olros/stats": "^1.3.0",
    "@prisma/client": "^5.2.0",
    "axios": "^1.5.0",
    "date-fns": "^2.30.0",
    "http-status-typed": "^1.0.1",
    "ics": "^3.4.0",
    "next": "^13.4.19",
    "prisma": "^5.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "react-hotkeys-hook": "^3.4.6",
    "react-rewards": "^2.0.4",
    "server-only": "^0.0.1",
    "string-similarity": "^4.0.4",
    "swr": "^2.1.5"
  },
  "devDependencies": {
    "@getbigger-io/prisma-fixtures-cli": "^2.0.1",
    "@types/node": "^20.5.7",
    "@types/react": "^18.2.21",
    "@types/string-similarity": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-next": "^13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  }
}
