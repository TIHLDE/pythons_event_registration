    "axios": "^1.4.0",
    "cookies-next": "^2.1.1",
    "date-fns": "^2.29.3",
    "http-status-typed": "^1.0.1",
    "ics": "^3.1.0",
    "lodash": "^4.17.21",
    "next": "^13.3.1",
    "prisma": "^4.13.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
