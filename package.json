{
  "name": "nextjs-with-typescript",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix src",
    "push": "npx prisma db push",
    "post-update": "echo \"codesandbox preview only, need an update\" && yarn upgrade --latest",
    "fixtures": "npx fixtures ./src/fixtures",
    "createdb": "docker run --name event_registration -e POSTGRES_PASSWORD=docker -p 5432:5432 -d postgres",
    "fresh": "docker rm -f -v event_registration && yarn createdb && sleep 1 && yarn push && npx prisma generate && yarn fixtures"
  },
  "dependencies": {
    "@emotion/cache": "11.10.1",
    "@emotion/react": "11.10.0",
    "@emotion/server": "11.10.0",
    "@emotion/styled": "11.10.0",
    "@mui/icons-material": "5.8.4",
    "@mui/material": "5.10.1",
    "@prisma/client": "^4.2.1",
    "axios": "^0.26.1",
    "date-fns": "^2.28.0",
    "http-status-typed": "^1.0.1",
    "lodash": "^4.17.21",
    "next": "12.2.5",
    "prisma": "^4.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.29.0",
    "react-hotkeys-hook": "^3.4.6",
    "safe-json-stringify": "^1.2.0",
    "swr": "^1.2.2",
    "ts-node": "^10.7.0"
  },
  "devDependencies": {
    "@getbigger-io/prisma-fixtures-cli": "^2.0.1",
    "@types/lodash": "^4.14.181",
    "@types/node": "^17.0.23",
    "@types/react": "18.0.17",
    "@types/safe-json-stringify": "^1.1.2",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "^8.22.0",
    "eslint-config-next": "^12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "typescript": "4.7.4"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  }
}
