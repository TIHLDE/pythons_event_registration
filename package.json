{
  "name": "tihlde-pythons",
  "version": "7.0.0",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "next dev",
    "build": "yarn db:generate && next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --fix src",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --skip-seed",
    "db:new-migration": "prisma migrate dev --skip-seed --name",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "docker:down": "docker-compose down",
    "docker:fresh": "yarn docker:down && yarn docker:start && sleep 1 && yarn db:migrate && yarn db:generate",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose stop"
  },
  "dependencies": {
    "@nextui-org/react": "^2.2.9",
    "@olros/stats": "^2.2.1",
    "@prisma/client": "^5.10.2",
    "@tanstack/react-query": "^5.27.5",
    "axios": "^1.6.7",
    "clsx": "^2.1.0",
    "date-fns": "^3.4.0",
    "framer-motion": "^11.0.3",
    "http-status-typed": "^1.0.1",
    "ics": "^3.7.2",
    "next": "^14.0.3",
    "prisma": "^5.10.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.0",
    "react-hotkeys-hook": "^4.5.0",
    "react-icons": "^5.0.1",
    "react-rewards": "^2.0.4",
    "server-only": "^0.0.1",
    "string-similarity": "^4.0.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.10",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/string-similarity": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.1.3",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.0",
    "typescript": "^5.4.2"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma",
    "seed": "tsx src/prisma/seed.ts"
  }
}
