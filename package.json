{
  "name": "tihlde-pythons",
  "version": "7.0.0",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "next dev",
    "build": "yarn db:generate && next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --fix src",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --skip-seed",
    "db:new-migration": "prisma migrate dev --skip-seed --name",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "docker:down": "docker-compose down",
    "docker:fresh": "yarn docker:down && yarn docker:start && sleep 1 && yarn db:migrate && yarn db:generate",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose stop"
  },
  "dependencies": {
    "@nextui-org/react": "^2.1.13",
    "@olros/stats": "^2.2.1",
    "@prisma/client": "^5.5.2",
    "@tanstack/react-query": "^4.35.0",
    "axios": "^1.5.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.4",
    "http-status-typed": "^1.0.1",
    "ics": "^3.4.0",
    "next": "^14.0.1",
    "prisma": "^5.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-hotkeys-hook": "^3.4.6",
    "react-icons": "^4.11.0",
    "react-rewards": "^2.0.4",
    "server-only": "^0.0.1",
    "string-similarity": "^4.0.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.36",
    "@types/react-dom": "^18.2.14",
    "@types/string-similarity": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-config-next": "^13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.0.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma",
    "seed": "ts-node --transpile-only --compiler-options {\"module\":\"CommonJS\"} src/prisma/seed.ts"
  }
}
